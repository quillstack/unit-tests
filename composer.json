{
    "name": "quillstack/unit-tests",
    "description": "A simple library for unit testing in PHP 8.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "quillstack",
        "php",
        "php8",
        "unit",
        "tests"
    ],
    "homepage": "https://quillstack.org/unit-tests",
    "authors": [
        {
            "name": "Radek Ziemniewicz",
            "email": "radek@quillstack.org"
        }
    ],
    "bin": ["bin/unit-tests"],
    "autoload": {
        "psr-4": {
            "Quillstack\\UnitTests\\": "src/",
            "Quillstack\\UnitTests\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "quillstack/local-storage": "^0.0.3",
        "quillstack/di": "^0.0.6",
        "quillstack/test-coverage": "^0.0.4"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        }
    }
}
